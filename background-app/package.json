{
  "name": "background-app",
  "private": true,
  "scripts": {
    "start": "MONGO_URL=mongodb://127.0.0.1:3001/meteor MONGO_OPLOG_URL=mongodb://127.0.0.1:3001/local meteor run --port 4000 --settings settings-local.json",
    "start:test": "MONGO_URL=mongodb://127.0.0.1:27017/test meteor run --port 4337 --settings settings-test.json",
    "debug": "MONGO_URL=mongodb://127.0.0.1:3001/meteor MONGO_OPLOG_URL=mongodb://127.0.0.1:3001/local meteor debug --settings settings-local.json",
    "start:windows": "cmd /c \"SET MONGO_URL=mongodb://127.0.0.1:3001/meteor&&meteor --port 4000 --settings settings-local.json\"",
    "deploy": "rm -rf node_modules && yarn install && cd .deploy && pm2-meteor deploy",
    "deploy:staging": "rm -rf node_modules && yarn install && cd .deploy-staging && pm2-meteor deploy"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.46",
    "babel-runtime": "^6.26.0",
    "deep-diff": "^0.3.4",
    "handlebars": "^4.0.5",
    "invariant": "^2.2.4",
    "lodash.get": "^4.4.2",
    "lodash.property": "^4.4.2",
    "meteor-node-stubs": "~0.2.0",
    "moment-timezone": "^0.5.7",
    "plio-util": "https://github.com/Pliohub/plio-util.git#devel",
    "pluralize": "^3.0.0",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "styled-components": "^3.2.6"
  }
}
